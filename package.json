{
  "name": "ask",
  "version": "3.0.0",
  "description": "ASK",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rm -rf dist",
    "assets:copy": "mkdir -p ../public/v3-files && cp -an assets ../public/v3-files/",
    "lint": "eslint src",
    "eslint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:check": "eslint --print-config .eslintrc |eslint-config-prettier-check",
    "tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "vite",
    "test": "TZ=UTC jest --watchAll --maxWorkers=70%",
    "jest": "TZ=UTC jest",
    "jest:staged": "npm run jest -- --watchAll=false --passWithNoTests --findRelatedTests",
    "test:ci": "better-npm-run ci",
    "test:ci:debug": "better-npm-run ci:debug",
    "deploy": "npm run build && npm run deploy-serviceworker && npm run assets:copy",
    "kill-chrome": "./bin/kill-orphaned-chrome.sh",
    "precommit": "lint-staged",
    "deploy-serviceworker": "better-npm-run deploy:serviceworker",
    "deploy-serviceworker-debug": "better-npm-run deploy:serviceworker:debug",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "jest-preview": "jest-preview"
  },
  "betterScripts": {
    "deploy:serviceworker": {
      "command": "bin/deploy-serviceworker.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:serviceworker:debug": {
      "command": "bin/deploy-serviceworker.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "ci": {
      "command": "jest --ci --reporters='jest-junit' --reporters=default --coverage --coverageDirectory=coverage --maxWorkers=${MAX_WORKERS:-70%} --silent",
      "env": {
        "NODE_ENV": "test",
        "TZ": "UTC",
        "CI": "true"
      }
    }
  },
